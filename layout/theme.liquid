      .dropdown-links::-webkit-scrollbar-track {
        background: #f1f1f1; /* color of the track */
      }
      
      .dropdown-links::-webkit-scrollbar-thumb {
        background: #888; /* color of the thumb */
        border-radius: 5px; /* roundness of the thumb */
      }
